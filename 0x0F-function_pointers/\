#include "function_pointers.h"
/**
 * main - main function
 */
int main(int argc, char *argv[])
{
	
	if(argc < 3)
	{
		printf("Error\n");
		return (98);
	}
	if(argv[2] != + || argv[2] != '-' || argv[2] != '*' || argv[2] != '/' || argv[2] != '%')
	{
		printf("Error\n");
		return (99);
	}
	if ((argv[2] == '/' && atoi(argv[3] == 0) || (argv[2] == '%' && atoi(argv[3] == 0))))
	{
		printf("Error\n");
		return (100);
	}
	return (get_op_func(argv[2])(atoi(argv[1], atoi(argv[3]))));
}
